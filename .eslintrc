{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true,
    "node": true,
    "commonjs ": true
  },
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:tailwindcss/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["import", "tailwindcss", "prettier"],
  "rules": {
    "no-duplicate-imports": "error",
    "import/no-unresolved": "error"
  },
  "settings": {},
  "overrides": []
}
